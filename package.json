  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^24.9.1",
    "babel-jest": "^25.5.1",
    "babel-preset-gatsby": "^0.2.36",
    "cli-glob": "^0.1.0",
